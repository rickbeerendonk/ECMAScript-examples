{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "arrowParens": "avoid",
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "overrides": [
    {
      "files": ["*.json"],
      "options": {
        "singleQuote": false
      }
    }
  ]
}
